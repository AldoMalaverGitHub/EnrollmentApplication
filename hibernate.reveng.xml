<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="db_enrollment" match-name="course"/>
  <table-filter match-catalog="db_enrollment" match-name="courseclass"/>
  <table-filter match-catalog="db_enrollment" match-name="courseclassattendance"/>
  <table-filter match-catalog="db_enrollment" match-name="courseclassdetail"/>
  <table-filter match-catalog="db_enrollment" match-name="enrollment"/>
  <table-filter match-catalog="db_enrollment" match-name="enrollmentcourseclass"/>
  <table-filter match-catalog="db_enrollment" match-name="enrollmentdetail"/>
  <table-filter match-catalog="db_enrollment" match-name="enrollmentdetailschedule"/>
  <table-filter match-catalog="db_enrollment" match-name="grade"/>
  <table-filter match-catalog="db_enrollment" match-name="professor"/>
  <table-filter match-catalog="db_enrollment" match-name="semester"/>
  <table-filter match-catalog="db_enrollment" match-name="service"/>
  <table-filter match-catalog="db_enrollment" match-name="student"/>
  <table-filter match-catalog="db_enrollment" match-name="studentaddress"/>
</hibernate-reverse-engineering>